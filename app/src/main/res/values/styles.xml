<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="some_id">

        <item name="android:textSize">
            24sp
        </item>

        <item name="android:letterSpacing">
            -0.04
        </item>

        <item name="android:textColor">
            #DEFFFFFF
        </item>

    </style>

    <style name="page_title">

        <item name="android:textSize">
            18sp
        </item>

        <item name="android:letterSpacing">
            -0.03
        </item>

        <item name="android:textColor">
            #DEFFFFFF
        </item>

    </style>

    <style name="InterRegularTextAppearance">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textColor">
            #DEFFFFFF
        </item>
    </style>
    <style name="countries">

        <item name="android:textSize">
            14sp
        </item>

        <item name="android:letterSpacing">
            -0.02
        </item>

        <item name="android:textColor">
            #DEFFFFFF
        </item>

    </style>

    <style name="icon_text">

        <item name="android:textSize">
            14sp
        </item>

        <item name="android:letterSpacing">
            -0.02
        </item>

        <item name="android:textColor">
            #9575CD
        </item>

    </style>

    <style name="SCBSwitch" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">#9575CD</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/second_white
        </item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">@color/main
        </item>
    </style>

</resources>